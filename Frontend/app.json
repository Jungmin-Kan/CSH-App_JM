{
  "expo": {
    "name": "청신한(일반 사용자)",
    "slug": "청신한(일반 사용자)",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_user.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon_user.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_user.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.csh.user"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon_user.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ]
    ]
  }
}
